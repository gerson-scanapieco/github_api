<!-- Commits -->
<table class="table table-condensed"> 
<thead> 
  <tr>
    <th>Author</th>
    <th>Message</th>
    <th>Commit Date</th>
    <th>Html_url</th>
    <th>Branch</th>
</thead>
<tbody>
<% @commits_list.each_value do |element| %>
  <% element.body.each do |commit| %>
    <tr>
      <% if commit.committer.class == Hashie::Mash %>
        <td><%= link_to nil, "http://github.com/#{commit.committer.login}" %></td>
      <% else %>
        <td><%= commit.commit.author.name %></td>
      <% end %>
      <% if commit.commit.class == Hashie::Mash %>
        <td><%= commit.commit.message %></td>
      <% else %>
        <td>Empty</td>
      <% end %>
      <% if commit.commit.author.class == Hashie::Mash %>
       <td><%= commit.commit.author.date %></td>
      <% else %>
        <td>Empty</td>
      <% end %>
      <td><%= link_to nil, "#{commit.html_url}" %></td>
      <td><%= @commits_list.key(element) %></td>
    </tr>
  <% end %>
<% end %>
</tbody>
</table>